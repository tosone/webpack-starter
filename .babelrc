{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "chrome": 52,
          "browsers": [
            "last 2 versions",
            "safari 7"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-transform-class-properties"
  ]
}
